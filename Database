Create Table User(
User_Id INT NOT NULL, 
First_Name VARCHAR(255),
Last_Name VARCHAR(255), 
Username VARCHAR(255) NOT NULL, 
Password VARCHAR(255) NOT NULL,
Email VARCHAR(255) NOT NULL,
Age INT, 
Weight INT,
Height INT, 
Calories INT,
Num_Meals INT,
Protein INT,
Carbs INT,
Fat INT,
Sugar INT,
Fiber INT,
CONSTRAINT User_PK PRIMARY KEY (User_Id)
);

Create Table Meals(
Meal_Id INT NOT NULL, 
User_Id INT NOT NULL,
Name VARCHAR(1000),
Calories INT,
Protein INT,
Carbs INT, 
Total_Fat INT,
Fiber INT, 
Sugar INT, 
CONSTRAINT Meals_PK PRIMARY KEY (Meal_Id),
FOREIGN KEY (User_Id) REFERENCES User(User_Id)
);

Create Table FoodsInMeal(
Meal_Id INT NOT NULL, 
Food_Id VARCHAR(10000) NOT NULL,
Serving INT,
Unit VARCHAR(1000),
FOREIGN KEY (Meal_Id) REFERENCES Meals(Meal_Id)
);

Create Table Inventory(
User_Id INT NOT NULL, 
Food_Id VARCHAR(10000) NOT NULL,
Serving INT,
Unit VARCHAR(1000),
FOREIGN KEY (User_Id) REFERENCES User(User_Id)
);

Create Table MealPlans(
MealPlan_Id INT NOT NULL, 
User_Id INT NOT NULL,
CONSTRAINT MealPlans_PK PRIMARY KEY (MealPlan_Id),
FOREIGN KEY (User_Id) REFERENCES User(User_Id)
);

Create Table MealsInPlan(
MealPlan_Id INT NOT NULL,
Meal_Id INT NOT NULL, 
FOREIGN KEY (MealPlan_Id) REFERENCES MealPlans(MealPlan_Id),
FOREIGN KEY (Meal_Id) REFERENCES Meals(Meal_Id)
);
